package javapackage.com;

public class IfConditions {
	
	public static void main (String[] args){
		
		int a=11;
		int b=13,c=20;
		
		if(a>b){
			
			System.out.println("a is greater");
			
		}
		else
		{
			System.out.println("b is greater");
			
		}
		if(c>a){
			System.out.println("c is greater");
		}
		
		else{
			System.out.println("b is greater");
		}
			
	}

}
